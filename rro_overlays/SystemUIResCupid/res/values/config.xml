<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Default rounded corner curve (a Bezier). Must match (the curved path in) rounded.xml.
         Note that while rounded.xml includes the entire path (including the horizontal and vertical
         corner edges), this pulls out just the curve.
     -->
    <string name="config_rounded_mask" translatable="false">M144,0c-7.8,0.7-15.6,1.3-23.3,2.1C98.4,4.2,76.9,8.8,57.5,20.4C29.6,37.2,14.3,63,7.3,93.8C4,108.1,2,130.4,0,144</string>

    <!-- Doze: does this device support STATE_DOZE?  -->
    <bool name="doze_display_state_supported">false</bool>

    <!-- Preferred refresh rate at keyguard, if supported by the display. Overrides
         keyguardMaxRefreshRate. -->
    <integer name="config_keyguardRefreshRate">60</integer>

    <!-- StatusBar Burn-in protection -->
    <bool name="config_statusBarBurnInProtection">true</bool>
    <integer name="config_shift_interval">40</integer>
</resources>
